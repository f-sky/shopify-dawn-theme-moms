<link rel="stylesheet" href="{{ 'section-main-page.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-rte.css' | asset_url }}" media="print" onload="this.media='all'">

<noscript>{{ 'section-main-page.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-rte.css' | asset_url | stylesheet_tag }}</noscript>
{{ 'page-signup-form.css' | asset_url | stylesheet_tag }}

<div class="page-width page-width--narrow">
  <h1 class="main-page-title page-title h0">
    {{ page.title | escape }}
  </h1>
  <p class="description">
    Please share a little about yourself so we can customize offers and make sure your time isnâ€™t wasted
  </p>
  <div class="rte">
    
    <!-- Begin Mailchimp Signup Form -->
    <div id="mc_embed_signup">
        <form 
            action="https://cbdformoms.us20.list-manage.com/subscribe/post?u=a1fd13016ffeba9b5dc4f7644&amp;id=072de18b10&amp;f_id=001a65e5f0" 
            method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_self"
            onsubmit="return validateSignupFrom()">
            <div id="mc_embed_signup_scroll">
                <div class="indicates-required"><span class="asterisk">*</span> indicates required</div>
                <div class="mc-field-group field-email">
                    <div class="field">
                        <input type="email" value="" name="EMAIL" class="required email field__input" id="mce-EMAIL" required placeholder="Your email">
                        <label class="field__label" for="mce-EMAIL">Your email <span class="asterisk">*</span></label>
                    </div>
                </div>
                <div class="mc-field-group field-names">
                    <div class="field">
                        <input type="text" value="" name="FIRSTNAME" class="field__input" id="mce-FIRSTNAME" placeholder="First Name">
                        <label class="field__label" for="mce-FIRSTNAME">First Name</label>
                    </div>
                    <div class="field">
                        <input type="text" value="" name="LASTNAME" class="field__input" id="mce-LASTNAME" placeholder="Last Name">
                        <label class="field__label" for="mce-LASTNAME">Last Name</label>
                    </div>
                </div>
                <div class="mc-field-group size1of2">
                    <div class="field">
                        <input type="number" name="AGE" class="field__input" value="" id="mce-AGE" placeholder="Age">
                        <label class="field__label" for="mce-AGE">Your Age</label>
                    </div>
                </div>
                <div class="mc-field-group size1of2">
                    <div class="field">
                        <input type="number" name="KIDSCOUNT" class="field__input" value="" id="mce-KIDSCOUNT" placeholder="Kids">
                        <label class="field__label" for="mce-KIDSCOUNT">How many kids</label>
                    </div>
                </div>
                <div class="mc-field-group">
                    <div class="field">
                        <input type="text" value="" name="CITY" class="field__input" id="mce-CITY" placeholder="City">
                        <label class="field__label" for="mce-CITY">City</label>
                    </div>
                </div>
                <div class="mc-field-group">
                    <div class="field" style="margin-bottom: 0;">
                        <input type="text" value="" name="FOCUS" class="field__input" id="mce-FOCUS" placeholder="Focus">
                        <label class="field__label" for="mce-FOCUS">Focus</label>
                    </div>
                </div>
                <div class="mc-field-group">
                    <div class="field" style="margin-bottom: 0;">
                        <input type="text" value="" name="INTEREST" class="field__input" id="mce-INTEREST" placeholder="Interest">
                        <label class="field__label" for="mce-INTEREST">Interest</label>                        
                    </div>
                </div>
                <div class="mc-field-group">
                    <fieldset>
                        <legend>What has been your focus?</legend>
                        <div class="focus_properties-wrapper">
                            <div>
                                <input type="checkbox" name="focus_properties" value="Anxiety" id="focus-Anxiety"><label for="focus-Anxiety">Anxiety</label>
                            </div>
                            <div>
                                <input type="checkbox" name="focus_properties" value="Chronic pain" id="focus-Chronic-pain"><label for="focus-Chronic-pain">Chronic pain</label>
                            </div>
                            <div>
                                <input type="checkbox" name="focus_properties" value="Depression" id="focus-Depression"><label for="focus-Depression">Depression</label>
                            </div>
                            <div>
                                <input type="checkbox" name="focus_properties" value="Difficulty with sleep" id="focus-Difficulty-with-sleep"><label for="focus-Difficulty-with-sleep">Difficulty with sleep</label>
                            </div>
                            <div>
                                <input type="checkbox" name="focus_properties" value="Lack of focus" id="focus-Lack-of-focus"><label for="focus-Lack-of-focus">Lack of focus</label>
                            </div>
                            <div>
                                <input type="checkbox" name="focus_properties" value="Other" id="focus-other"><label for="focus-other">Other</label>
                            </div>
                        </div>                        
                    </fieldset>
                </div>

                <div class="mc-field-group">
                    <fieldset>
                        <legend>What are you most interested in?</legend>
                        <div class="focus_properties-wrapper">
                            <div>
                                <input type="checkbox" name="interest_properties" value="Tips for parents" id="interest-Tips-for-parents"><label for="interest-Tips-for-parents">Tips for parents</label>
                            </div>
                            <div>
                                <input type="checkbox" name="interest_properties" value="Being a product tester" id="interest-product-tester"><label for="interest-product-tester">Being a product tester</label>
                            </div>
                            <div>
                                <input type="checkbox" name="interest_properties" value="Being a brand ambassador" id="interest-brand-ambassador"><label for="interest-brand-ambassador">Being a brand ambassador</label>
                            </div>
                            <div>
                                <input type="checkbox" name="interest_properties" value="Meeting new friends" id="interest-new-friends"><label for="interest-interest-new-friends">Meeting new friends</label>
                            </div>
                            <div>
                                <input type="checkbox" name="interest_properties" value="Exclusive product launches" id="interest-Exclusive-product-launches"><label for="interest-Exclusive-product-launches">Exclusive product launches</label>
                            </div>
                            <div>
                                <input type="checkbox" name="interest_properties" value="Everything and anything!" id="interest-Everything-anything"><label for="interest-Everything-anything">Everything and anything!</label>
                            </div>
                        </div>                        
                    </fieldset>
                </div>

                <div hidden="true"><input type="hidden" name="tags" value="3122630"></div>
                <div id="mce-responses" class="clear">
                    <div class="response" id="mce-error-response" style="display:none"></div>
                    <div class="response" id="mce-success-response" style="display:none"></div>
                </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_a1fd13016ffeba9b5dc4f7644_072de18b10" tabindex="-1" value=""></div>
                <div class="clear">
                    <button type="submit" name="subscribe" id="mc-embedded-subscribe" class="button">
                        Join the club
                        <div class="loading-overlay__spinner hidden">
                            <svg aria-hidden="true" focusable="false" role="presentation" class="spinner" viewBox="0 0 66 66" xmlns="http://www.w3.org/2000/svg">
                            <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                            </svg>
                        </div>
                    </button>
                </div>
            </div>
        </form>
    </div>

    <!--End mc_embed_signup-->

  </div>
</div>

{{ 'page-signup-form.js' | asset_url | script_tag }}

